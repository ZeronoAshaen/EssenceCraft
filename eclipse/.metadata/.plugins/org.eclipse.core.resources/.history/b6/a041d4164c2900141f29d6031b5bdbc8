package com.zerono.essencecraft.items;

import com.zerono.essencecraft.createiveTab.CreativeTabEC;

import net.minecraft.item.Item;

public class ItemInertDust extends ItemEC
{
    public ItemInertDust()
    {
        super();
        this.setMaxStackSize(64);
        this.setUnlocalizedName("inertDust");
        this.setCreativeTab(CreativeTabEC.EC_ITEMS_TAB);
    }
}
